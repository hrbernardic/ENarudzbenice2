{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "enarudzbenice2-web-en2-web": {
      "root": "apps/enarudzbenice2-web/en2-web/",
      "sourceRoot": "apps/enarudzbenice2-web/en2-web/src",
      "projectType": "application",
      "prefix": "en2",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/enarudzbenice2-web/en2-web",
            "index": "apps/enarudzbenice2-web/en2-web/src/index.html",
            "main": "apps/enarudzbenice2-web/en2-web/src/main.ts",
            "polyfills": "apps/enarudzbenice2-web/en2-web/src/polyfills.ts",
            "tsConfig": "apps/enarudzbenice2-web/en2-web/tsconfig.app.json",
            "assets": [
              "apps/enarudzbenice2-web/en2-web/src/favicon.ico",
              "apps/enarudzbenice2-web/en2-web/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/enarudzbenice2-web/en2-web/src/main.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/enarudzbenice2-web/en2-web/src/environments/environment.ts",
                  "with": "apps/enarudzbenice2-web/en2-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "enarudzbenice2-web-en2-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "enarudzbenice2-web-en2-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "enarudzbenice2-web-en2-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/enarudzbenice2-web/en2-web/src/test.ts",
            "polyfills": "apps/enarudzbenice2-web/en2-web/src/polyfills.ts",
            "tsConfig": "apps/enarudzbenice2-web/en2-web/tsconfig.spec.json",
            "karmaConfig": "apps/enarudzbenice2-web/en2-web/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/enarudzbenice2-web/en2-web/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "apps/enarudzbenice2-web/en2-web/src/favicon.ico",
              "apps/enarudzbenice2-web/en2-web/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/enarudzbenice2-web/en2-web/tsconfig.app.json",
              "apps/enarudzbenice2-web/en2-web/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "enarudzbenice2-web-en2-web-e2e": {
      "root": "apps/enarudzbenice2-web/en2-web-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/enarudzbenice2-web/en2-web-e2e/protractor.conf.js",
            "devServerTarget": "enarudzbenice2-web-en2-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "enarudzbenice2-web-en2-web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/enarudzbenice2-web/en2-web-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/schematics",
    "packageManager": "npm"
  },
  "defaultProject": "enarudzbenice2-web-en2-web"
}
